{
  "name": "backend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "dev": "nest start --watch",
    "start": "nest start",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/backend/src/main",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "gen:module": "./scripts/gen_module.sh"
  },
  "dependencies": {
    "@nestjs/common": "^8.0.0",
    "@nestjs/config": "^1.0.2",
    "@nestjs/core": "^8.0.0",
    "@nestjs/passport": "^8.0.1",
    "@nestjs/platform-express": "^8.0.0",
    "@octokit/auth-oauth-user": "^2.0.4",
    "@octokit/core": "^4.1.0",
    "@pc/abi": "*",
    "@pc/common": "*",
    "@pc/database": "*",
    "@pc/rpcaas-client": "file:../rpcaas-client",
    "@prisma/client": "^3.4.2",
    "@types/json-schema": "^7.0.11",
    "@types/passport-http-bearer": "^1.0.37",
    "axios": "^0.24.0",
    "bs58": "^5.0.0",
    "express": "^4.18.1",
    "firebase-admin": "^10.0.0",
    "form-data": "^4.0.0",
    "kysely": "^0.20.0",
    "libsodium-wrappers": "^0.7.10",
    "luxon": "^3.2.1",
    "mailgun.js": "^7.0.2",
    "nanoid": "^3.1.30",
    "near-abi-client-js": "https://github.com/near/near-abi-client-js.git#13cc45a2a0722159c263112f43aa398364da889e",
    "nestjs-pino": "^3.1.1",
    "passport": "^0.6.0",
    "passport-http": "^0.3.0",
    "passport-http-bearer": "^1.0.1",
    "pg": "^8.8.0",
    "pg-hstore": "^2.3.4",
    "pino-http": "^8.2.1",
    "prom-client": "^14.1.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "sequelize": "^6.29.0",
    "verror": "^1.10.1",
    "web3.storage": "^4.4.0",
    "zod-validation-error": "^0.2.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.2.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.1",
    "@types/luxon": "^3.0.0",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.0.0",
    "@types/passport-http": "^0.3.9",
    "@types/pg": "^8.6.5",
    "@types/supertest": "^2.0.11",
    "@types/verror": "^1.10.5",
    "jest": "^27.0.6",
    "pino-pretty": "^9.1.1",
    "supertest": "^6.2.4",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.4.0",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.8.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "roots": [
      "<rootDir>",
      "../abi/"
    ],
    "modulePaths": [
      "<rootDir>",
      "<rootDir>/src"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "<rootDir>/coverage",
    "testEnvironment": "node"
  }
}
